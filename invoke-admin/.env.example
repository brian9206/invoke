# Invoke Admin Environment Configuration

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=invoke_db
DB_USER=postgres
DB_PASSWORD=invoke_password_123
MIGRATIONS_DIR=/path/to/invoke/database

# S3-compatible Object Storage Configuration (MinIO by default)
S3_ENDPOINT=localhost
S3_PORT=9000
S3_USE_SSL=false
S3_ACCESS_KEY=invoke-minio
S3_SECRET_KEY=invoke-minio-password-123
S3_BUCKET=invoke-packages

# Next.js Configuration
NEXTAUTH_URL=http://localhost:3000
JWT_SECRET=your_super_secret_jwt_key_change_this_in_production

# Cloudflare Turnstile (CAPTCHA)
# Get your keys from https://dash.cloudflare.com/
# For testing, use: 1x00000000000000000000AA (always passes)
TURNSTILE_SITE_KEY=1x00000000000000000000AA
TURNSTILE_SECRET_KEY=1x0000000000000000000000000000000AA

# Service URLs
EXECUTION_URL=http://localhost:3001
